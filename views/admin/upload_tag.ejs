<%- include('master_header'); -%>
<style>
  .form-control {
    -webkit-box-shadow: inset 0 0px 0 rgba(0,0,0,0.075);
    box-shadow: inset 0 px 0 rgba(0,0,0,0.075);
}
input[type="file"]{
   -webkit-appearance: none;
   text-align: left;
   -webkit-rtl-ordering:  left;
}
input[type="file"]::-webkit-file-upload-button{
   -webkit-appearance: none;
   float: right;
   margin: 0 0 0 10px;
   border: 1px solid #aaaaaa;
   border-radius: 4px;
   background-image: -webkit-gradient(linear, left bottom, left top, from(#d2d0d0), to(#f0f0f0));
   background-image: -moz-linear-gradient(90deg, #d2d0d0 0%, #f0f0f0 100%);
}
</style>
<div class="jumbotron">
  <h1>Upload Tag CSV</h1>
  
  <!-- <a class="btn btn-lg btn-primary" href="/docs/4.5/components/navbar/" role="button">View navbar docs
      &raquo;</a> -->
</div>
<form action="/api/gateway/UploadTagToDatabase" method="POST" enctype="multipart/form-data">
  <div class="form-group row">
    <div class="col-sm-6">
    <input type="file" id="myFile" name="filename" style="width:100%;border: solid;border-width: 0.05rem;border-color: grey;border-radius: 5px;" >
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-10">
      <a href="/api/gateway/index/" style="margin-right: 0.5rem; margin-left: 0.5rem;" class="float-right btn btn-primary">Back</a>
      <button type="submit" class="float-right btn btn-primary">Upload</button>
    </div>
  </div>
  </form>

  <script src="https://code.jquery.com/jquery-1.9.1.min.js"></script>
  <script>
    $(document).ready(function() {
     var url=window.location.href;
     var urlarray=url.split('=');
     if(urlarray[1]=="1"){
      alert("Upload Sucessfuly");
     }
  });
  
  </script>
<%- include('master_footer'); -%>