<%- include('master_header'); -%>
          <a href="/api/gateway/details/<%= readerId%>" style="margin-left:0.2rem" class="btn btn-primary float-right">Back</a>
          <a href="/api/gateway/AddAntenna/<%= readerId%>" class="btn btn-primary float-right">Add Antenna</a>
<table class="table">
    <thead class="thead-dark">
      <tr>
        <th scope="col">#</th>
        <th scope="col">Altitude</th>
        <th scope="col">Latitude</th>
        <th scope="col">Longitude</th>
        <th scope="col">Rack No.</th>
        <th scope="col">AntennaPort</th>
        <th scope="col">Rack Mounted</th>
        <th scope="col">Actions</th>
      </tr>
    </thead>
    <tbody>
      <% let counter=1 %>
      <% antenna.forEach(item=>{ %>
        <tr>
            <th scope="row"><%= counter++ %></th>
            <td><%= item.Altitude %></td>
            <td><%= item.Latitude %></td>
            <td><%= item.Longitude %></td>
            <td><%= item.RackNo %></td>
            <td><%= item.AntennaPort %></td>
            <td><%= item.RackMounted %></td>
            <td>
                
                <a href="/api/gateway/edit_antenna/<%= item.Id %>" class="btn btn-warning">Edit</a>
                <a href="/api/gateway/delete_antenna/<%= item.Id %>" class="btn btn-danger">Delete</a>
                
            </td>
          </tr>
          
      <% }) %>
      
    </tbody>
  </table>
  
  <%- include('master_footer'); -%>